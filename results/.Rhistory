# Calculate Speedup
my_data <- read.csv('AcerPC.csv')
my_data$Speedup <- my_data$Serial_Time / my_data$Parallel_Time
my_data
# Plot Speedup vs. Grid Size
ggplot(data = my_data, aes(x = Grid_Size, y = Speedup)) +
geom_line() +
geom_point() +
labs(title = 'Speedup vs. Grid Size',
x = 'Grid Size',
y = 'Speedup') +
theme_minimal()
